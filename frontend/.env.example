# PlantPal Frontend Environment Variables
# Copy this file to .env and fill in your actual values

# API Configuration
REACT_APP_API_URL=http://localhost:8000

# AWS Configuration
REACT_APP_AWS_REGION=ap-southeast-1

# AWS Cognito Configuration
REACT_APP_USER_POOL_ID=your-region_xxxxxxxxx
REACT_APP_USER_POOL_CLIENT_ID=your-client-id-here
REACT_APP_COGNITO_DOMAIN=your-app-name.auth.region.amazoncognito.com

# OAuth Redirect URLs
REACT_APP_REDIRECT_SIGN_IN=http://localhost:3000/callback
REACT_APP_REDIRECT_SIGN_OUT=http://localhost:3000/