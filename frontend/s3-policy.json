{
        "Version": "2012-10-17",
        "Statement": [
            {
                "Effect": "Allow",
                "Action": [
                    "s3:PutObject",
                    "s3:PutObjectAcl",
                    "s3:GetObject",
                    "s3:ListBucket"
                ],
                "Resource": [
                    "arn:aws:s3:::plantpal-frontend-bucket",
                    "arn:aws:s3:::plantpal-frontend-bucket/*"
                ]
            }
        ]
    }